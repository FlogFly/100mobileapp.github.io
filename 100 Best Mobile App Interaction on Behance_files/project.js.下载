    175: function(t, e, n) {
        "use strict";
        var i = n(45), s = n.n(i), o = n(37), r = n.n(o);
        e.a = r.a.extend({
            init: function() {
                this._super.apply(this, arguments), this.listenTo(this._model, "appreciatedOn", this._appreciationUpdate);
            },
            render: function() {
                var t = this._model.get("appreciatedOn"), e = this._model.get("canUnappreciate");
                t && this._showAppreciation(), this.$view.toggleClass("can-unappreciate", e), this._bindAppreciationClick();
            },
            _bindAppreciationClick: function() {
                var t = this;
                this.$view.on("click", function() {
                    var e = Boolean(t._model.get("appreciatedOn")), n = t._model.get("canUnappreciate");
                    e ? n && t._controller.unappreciate() : t._controller.appreciate(t.$view.data("appreciate-from"));
                });
            },
            _appreciationUpdate: function() {
                this._model.get("appreciatedOn") ? this._showAppreciation() : this._resetAppreciation();
            },
            _showAppreciation: function() {
                var t = this._model.get("newAppreciation"), e = this._model.get("appreciatedOn");
                this.$view.data("adobe-analytics", "UnappreciateClick"), t || !e ? this.$view.addClass("thanks") : this.$view.addClass("appreciated").find(".js-appreciation-date").text(s.a.unix(e).format("LL"));
            },
            _resetAppreciation: function() {
                this.$view.data("adobe-analytics", "AppreciateClick").removeClass("appreciated thanks").find(".js-appreciation-date").text("");
            }
        });
    },
