    248: function(t, e, n) {
        "use strict";
        var i = n(2), s = n.n(i), o = n(40), r = n.n(o), a = n(4), l = n.n(a), c = n(141);
        e.a = l.a.extend({
            init: function(t, e, n, i, s, o, r, a) {
                this._$scrollContext = t, this._$socialButtons = e, this._$sidebarSpacer = n, this._sidebarWidth = this._$sidebarSpacer.innerWidth(), 
                this._isPopup = i, this._$projectView = s, this._$sidebarGroup = o, this._$projectWrap = this._$scrollContext.find(".js-project-wrap"), 
                this._$window = r, this._interval = a || 1e3, this._isPopup && (this._topPadding = this._$projectWrap.position().top, 
                this._windowOffset = this._$projectWrap.offset().top - this._topPadding);
            },
            bind: function() {
                var t = this;
                r.a.is("desktop") && t._initBindingsAndTimer(), r.a.on("desktop:enter", function() {
                    t._initBindingsAndTimer();
                }), r.a.on("desktop:exit", function() {
                    t._removeBindingAndTimer();
                });
            },
            _initBindingsAndTimer: function() {
                var t = this;
                t._calcHeights(), t._checkHeight(), t._positionSidebar(), t._timerId = setInterval(t._checkHeight.bind(t), t._interval), 
                t._$socialButtons.removeClass("hide"), t._$window.on("resize.project-lib-stickySocial", function() {
                    t._scrollContextHeight = t._$scrollContext.height(), t._positionSidebar();
                }), t._$scrollContext.on("scroll.project-lib-stickySocial", function() {
                    t._positionSidebar();
                });
            },
            _getNavHeight: function() {
                return this._isPopup ? 0 : c.default.getHeight() + s()(".js-live-banner").height();
            },
            _calcHeights: function() {
                this._isPopup || (this._topPadding = Math.round(this._$projectView.offset().top - this._getNavHeight())), 
                this._topSpacing = this._isPopup ? this._topPadding : this._topPadding + this._getNavHeight(), 
                this._socialHeight = this._$socialButtons.outerHeight(), this._topOffset = this._topSpacing + this._socialHeight, 
                this._scrollContextHeight = this._$scrollContext.height();
            },
            _checkHeight: function() {
                var t = this._$projectView.height() + this._topSpacing, e = this._$sidebarGroup.outerHeight();
                t === this._bottomOfProject && e === this._sidebarGroupHeight || (this._bottomOfProject = t, 
                this._sidebarGroupHeight = e, this._positionSidebar());
            },
            _positionSidebar: function() {
                var t = this._$scrollContext.scrollTop();
                this._bottomOfProject = this._$projectView.height() + this._topSpacing, this._sidebarGroupHeight = this._$sidebarGroup.outerHeight(), 
                this._positionSidebarGroup(t), this._positionSocialButtons(t);
            },
            _positionSidebarGroup: function(t) {
                var e = this._sidebarGroupHeight + t + this._topSpacing + this._socialHeight;
                e > this._bottomOfProject ? this._$sidebarGroup.css({
                    bottom: "",
                    position: "absolute",
                    top: t - this._topPadding + (this._bottomOfProject - e)
                }) : t > this._topPadding ? this._$sidebarGroup.css({
                    bottom: "",
                    position: "fixed",
                    width: this._sidebarWidth,
                    top: this._getNavHeight()
                }) : this._$sidebarGroup.css({
                    bottom: "",
                    position: "",
                    top: ""
                });
            },
            _positionSocialButtons: function(t) {
                var e = t + this._scrollContextHeight, n = this._sidebarGroupHeight + this._$sidebarGroup.offset().top + (this._isPopup ? t - this._windowOffset : 0), i = n + this._socialHeight;
                this._bottomOfProject < e ? this._$socialButtons.css({
                    position: "absolute",
                    top: "",
                    bottom: 0
                }) : i >= e ? this._$socialButtons.css({
                    position: "absolute",
                    top: n - this._topSpacing,
                    bottom: ""
                }) : this._$socialButtons.css({
                    position: "fixed",
                    top: "",
                    bottom: 0
                });
            },
            _removeBindingAndTimer: function() {
                this._$window.add(this._$scrollContext).off(".project-lib-stickySocial"), clearInterval(this._timerId);
            },
            unbind: function() {
                r.a.off(null, null, this), this._removeBindingAndTimer();
            }
        });
    },
